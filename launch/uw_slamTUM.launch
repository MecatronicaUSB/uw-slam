<!-- TUM RGBD DATASET -->
<launch>
    <!-- UW-SLAM main thread-->
    <node pkg="uw_slam" type="main_uw_slam" name="main"  
        args="-d    /home/fabiomorales/Documents/datasets/TUM-DATASET/rgbd_dataset_freiburg2_xyz/rgb/
              -c    /home/fabiomorales/catkin_ws/src/uw-slam/calibration/calibrationTUM.xml
              --TUM /home/fabiomorales/Documents/datasets/TUM-DATASET/rgbd_dataset_freiburg2_xyz/groundtruth.txt
              -p    /home/fabiomorales/Documents/datasets/TUM-DATASET/rgbd_dataset_freiburg2_xyz/depth/
              -s 210
              "
        clear_params="true" 
        output="screen"/>
        
    <!-- Rviz visualizer-->
    <node pkg="rviz" type="rviz" name="rviz"/>

    <node pkg="tf" type="static_transform_publisher" name="world_coordinate_frame" 
        args="0 0 0 0 0 0 world world_coordinate 100" />
</launch>
