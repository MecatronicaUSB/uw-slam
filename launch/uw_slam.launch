<!-- TUM RGBD DATASET -->
<launch>
    <!-- UW-SLAM main thread-->
    <node pkg="uw_slam" type="main_uw_slam" name="main"  
        args="-d /home/fabiomorales/Documents/datasets/TUM-DATASET/rgbd_dataset_freiburg2_rpy/rgb/
              -c /home/fabiomorales/catkin_ws/src/uw-slam/sample/calibration.xml  
              --TUM /home/fabiomorales/Documents/datasets/TUM-DATASET/rgbd_dataset_freiburg2_rpy/groundtruth.txt             
              -s 0"
        clear_params="true" 
        output="screen"/>
        
    <!-- Rviz visualizer-->
    <node pkg="rviz" type="rviz" name="rviz"/>
</launch>

